{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "navigationUrls": [
    "/**",
    "!/api/**",
    "!/about",
    "!/privacy-policy",
    "!/terms-and-conditions",
    "!/contact",
    "!/blogDetails/**",
    "!/main-styles.css/**",
    "!/assets/**"
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assests/**",
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": ["/api/getBlogs**"],
      "cacheConfig": {
        "maxAge": "1h",
        "strategy": "performance",
        "maxSize": 4
      }
    }
  ]
}
